\global \@namedef {allusevalue@alldef:Spolynomial}{17}
\global \@namedef {usevalue@namedef:Spolynomial}{($S$-polynomial)}
\global \@namedef {usevalue@predef:Spolynomial}{ \begin {tcolorbox}[statement={deftcol}{defbcol}, title={\underLine {\hyperref [def:Spolynomial]{\statementnameusevalue {def}~\ref *{def:Spolynomial}}}:~($S$-polynomial)~【Preview】}]  For $f, g \in R \setminus \left \{ 0 \right \}$, we define the \textit {$S$-polynomial} of $f$ and $g$ by \begin {align} S(f,g) = \frac {\mathrm {lcm}(\mathrm {in}(f), \mathrm {in}(g))}{\mathrm {init}(f)} f - \frac {\mathrm {lcm}(\mathrm {in}(f), \mathrm {in}(g))}{\mathrm {init}(g)} g \end {align} Here, $\mathrm {lcm}(\mu , \nu )$ is the least common multiple of monomials $\mu $ and $\nu $.  \end {tcolorbox} }
\global \@namedef {usevalue@redef:Spolynomial}{ \begin {tcolorbox}[statement={deftcol}{defbcol}, title={\underLine {\hyperref [def:Spolynomial]{\statementnameusevalue {def}~\ref *{def:Spolynomial}}}:~($S$-polynomial)~【Recall】}]  For $f, g \in R \setminus \left \{ 0 \right \}$, we define the \textit {$S$-polynomial} of $f$ and $g$ by \begin {align} S(f,g) = \frac {\mathrm {lcm}(\mathrm {in}(f), \mathrm {in}(g))}{\mathrm {init}(f)} f - \frac {\mathrm {lcm}(\mathrm {in}(f), \mathrm {in}(g))}{\mathrm {init}(g)} g \end {align} Here, $\mathrm {lcm}(\mu , \nu )$ is the least common multiple of monomials $\mu $ and $\nu $.  \end {tcolorbox} }
\global \@namedef {allusevalue@alllem:S-polycomb}{19}
\global \@namedef {usevalue@prelem:S-polycomb}{ \begin {tcolorbox}[statement={lemtcol}{lembcol}, title={\underLine {\hyperref [lem:S-polycomb]{\statementnameusevalue {lem}~\ref *{lem:S-polycomb}}}~【Preview】}]  Let $\mu \in \mathrm {Mon}(R)$, $g_1, \cdots , g_t \in R \setminus \left \{ 0 \right \}$, $c_1, \cdots , c_t \in K$. Assume that $\mathrm {in}(g_1), \cdots , \mathrm {in}(g_t) = \mu $. If $\displaystyle {\mathrm {in}\left ( \sum _i c_i g_i \right ) < \mu }$, then $\displaystyle {\sum _i c_i g_i}$ can be written as a linear combination of $S(g_i, g_j)$'s with coefficients in $K$.  \end {tcolorbox} }
\global \@namedef {usevalue@relem:S-polycomb}{ \begin {tcolorbox}[statement={lemtcol}{lembcol}, title={\underLine {\hyperref [lem:S-polycomb]{\statementnameusevalue {lem}~\ref *{lem:S-polycomb}}}~【Recall】}]  Let $\mu \in \mathrm {Mon}(R)$, $g_1, \cdots , g_t \in R \setminus \left \{ 0 \right \}$, $c_1, \cdots , c_t \in K$. Assume that $\mathrm {in}(g_1), \cdots , \mathrm {in}(g_t) = \mu $. If $\displaystyle {\mathrm {in}\left ( \sum _i c_i g_i \right ) < \mu }$, then $\displaystyle {\sum _i c_i g_i}$ can be written as a linear combination of $S(g_i, g_j)$'s with coefficients in $K$.  \end {tcolorbox} }
